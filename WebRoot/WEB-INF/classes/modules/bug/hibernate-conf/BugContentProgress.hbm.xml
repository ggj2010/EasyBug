<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.gaoguangjin.bug.entity">
	<class name="BugContentProgress" table="bug_content_progress">

		<id name="id" column="id" type="java.lang.String">
			<generator class="uuid.hex"></generator>
		</id>
		
		<property name="name" column="name" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="100">
		</property>

		
		<property name="flag" column="flag" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="100">
		</property>
		
		<property name="memo" column="memo" update="true"
			insert="true" type="text" not-null="false" unique="false"
			>
		</property>
		
		<property name="status" column="status" update="true"
			insert="true" type="java.lang.String" not-null="false" unique="false"
			length="100">
		</property>
		
		<property name="createDate" column="create_date"
			update="true" insert="true" type="java.util.Date" not-null="false"
			unique="false" length="0">
		</property>

		<!-- Many-To-One attributes -->
		<many-to-one name="fromUser" not-null="false" update="true"  lazy="false"
			insert="true" cascade="none">
			<column name="from_user" />
		</many-to-one>
		<many-to-one name="toUser" not-null="false" update="true"  lazy="false"
			insert="true" cascade="none">
			<column name="to_user" />
		</many-to-one>
		
		<many-to-one name="bugContent" not-null="false" update="true"  lazy="false"
			insert="true" cascade="none">
			<column name="bug_content" />
		</many-to-one>
		
		<many-to-one name="project" not-null="false" update="true"  lazy="false"
			insert="true" cascade="none">
			<column name="project" />
		</many-to-one>
		<many-to-one name="preid" not-null="false" update="true"  lazy="false"
			insert="true" cascade="none">
			<column name="preid" />
		</many-to-one>


	</class>
</hibernate-mapping>